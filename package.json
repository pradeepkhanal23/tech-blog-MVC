{
  "name": "tech-blog-mvc",
  "version": "1.0.0",
  "description": "full stack tech blog website",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "dev": "concurrently \"npm run watch:tailwind\" \"nodemon server.js\"   ",
    "seed": "node seeds/index.js",
    "tailwind:css": "postcss public/css/tailwind.css -o public/css/style.css",
    "watch:tailwind": "postcss public/css/tailwind.css -o public/css/style.css --watch",
    "migrate": "npx sequelize-cli db:migrate",
    "setup": "npm run migrate && npm run seed && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pradeepkhanal23/tech-blog-MVC.git"
  },
