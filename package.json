  },
  "homepage": "https://github.com/pradeepkhanal23/tech-blog-MVC#readme",
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "bcrypt": "^5.1.1",
    "connect-session-sequelize": "^7.1.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.18.0",
    "pg": "^8.12.0",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "sequelize": "^6.37.3",
    "tailwindcss": "^3.4.3"
  },
